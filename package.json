{
  "name": "@playkit-js/info",
  "version": "3.1.0",
  "main": "dist/playkit-info.js",
  "private": false,
  "bugs": {
    "url": "https://github.com/kaltura/playkit-js-info/issues"
  },
  "homepage": "https://github.com/kaltura/playkit-js-info#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaltura/playkit-js-info.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "conventional-github-releaser": "3.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cypress": "^12.0.1",
    "documentation": "^13.2.5",
    "husky": "^8.0.1",
    "kaltura-player-js": "https://github.com/kaltura/kaltura-player-js.git#master",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "sass": "^1.52.3",
    "sass-loader": "^12.6.0",
    "standard-version": "^9.3.2",
    "style-loader": "1.3.0",
    "ts-loader": "^9.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "peerDependencies": {
    "kaltura-player-js": "https://github.com/kaltura/kaltura-player-js.git#master"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --mode development",
    "watch": "webpack --progress --color --watch --mode development",
    "cypress:open": "cypress open",
    "test:watch": "yarn run build && cp ./dist/playkit-info.js ./cypress/public && yarn run cypress:open",
    "test": "cypress run",
    "release": "standard-version",
    "pushTaggedRelease": "git push --follow-tags --no-verify origin master",
    "eslint": "eslint . --color",
    "commit:dist": "git add --force --all dist && (git commit -m 'chore: update dist' || exit 0)",
    "docs:generate": "documentation build flow-typed/** src/** -f md -o docs/configuration.md",
    "docs:serve": "documentation serve flow-typed/** src/** --watch",
    "precommit": "lint-staged",
    "prettier:fix": "prettier --write ."
  },
  "keywords": [
    "info",
    "kaltura",
    "player",
    "playkit-js",
    "playkit-js-info",
    "html5 player"
  ],
  "dependencies": {
    "@playkit-js/common": "^1.0.11",
    "@playkit-js/ui-managers": "^1.3.0",
    "sanitize-html": "^2.7.1"
  },
  "kaltura": {
    "name": "playkit-js-info",
    "dependencies": {
      "playkit-ui-managers": "1.3.0"
    }
  }
}
